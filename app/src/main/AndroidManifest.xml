<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dabinu.apps.walletapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/theperfectlogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GeneralActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dabinu.apps.CreditActivity"
            android:label="@string/title_activity_credit_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.dabinu.apps.DebtActivity"
            android:label="@string/title_activity_debt_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.dabinu.apps.NewField"></activity>

        <service android:name=".TheDailyBread"
            android:exported="false"/>
    </application>

</manifest>