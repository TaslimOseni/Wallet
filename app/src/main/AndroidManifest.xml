<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dabinu.apps.walletapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_tas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dabinu.apps.Credits"
            android:label="@string/title_activity_credit_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.dabinu.apps.DebtList"
            android:label="@string/title_activity_debt_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.dabinu.apps.NewField"></activity>
    </application>

</manifest>